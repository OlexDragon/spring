<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%n%-5p%n%d [%t] %C{2} %M (%F:%L) - %m%xEx%n" />
        </Console>
    </Appenders>

    <Loggers>
<!-- 	<Logger name="org.springframework" level="all"> -->
<!-- 			<AppenderRef ref="STDOUT" /> -->
<!-- 	</Logger> -->

        <Root level="all">
			<AppenderRef ref="STDOUT" />
         </Root>
    </Loggers>
</Configuration>
