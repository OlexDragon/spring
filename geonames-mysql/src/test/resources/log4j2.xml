<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%-5p %d [%t] %C{2} %M (%F:%L) - %m%xEx%n" />
        </Console>
    </Appenders>

    <Loggers>
    	<logger name="jk.geonames.mysql.entities.address.repositopies.ContinentsRepositoryTest" level="all" />
        <Root level="error">
			<AppenderRef ref="STDOUT" />
         </Root>
    </Loggers>
</Configuration>
